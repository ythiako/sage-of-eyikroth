  id: Round4_HumansAndDwarves
  genericConflict: 0
  descriptionLines:
  - faction: 1
    line:
      en: Our mines recently caved in because of some earthquake, although we don't
        know why. Our economy is plummeting because of it!
      tr: 
  - faction: 5
    line:
      en: Humans are cheating on trading. We ask o' them valuable gems and minerals,
        but all they feed tae us are brass and rubies!
      tr: 
  aSummary:
    faction: 1
    line:
      en: We need our mines cleared so we can mine it again!
      tr: 
  aDecision:
    id: Round4_SideWithHumans
    unlockingFlag: 
    text:
      en: Dwarves will help you
      tr: 
    outcomes:
      m_keys: 0100000005000000
      m_values: 0a000000f6ffffff
  bSummary:
    faction: 5
    line:
      en: We demand payment NOW!
      tr: 
  bDecision:
    id: Round4_SideWithDwarves
    unlockingFlag: 
    text:
      en: Receive compensation
      tr: 
    outcomes:
      m_keys: 0100000005000000
      m_values: f6ffffff0a000000
  optionalDecisions:
  - id: Round4_LetGnomesHandle
    unlockingFlag: HumansAndDwarves_OptionalDecision_Round4
    text:
      en: Gnomes can clear the mines
      tr: 
